CREATE VIEW RECIPEVIEW AS
SELECT P.PRODUCT_NO, PNAME, MNAME, QUANTITY
FROM PRODUCT AS P
JOIN RECIPE AS R  ON R.PRODUCT_NO = P.PRODUCT_NO
JOIN MATERIAL AS M ON M.MATERIAL_NO = R.MATERIAL_NO
